draw_set_valign(fa_middle);  //Vertical alignment
draw_set_font(fntMainMed);
menu_x=room_width/2; 
menu_y=room_height/2;
for (i=0;i&lt;array_length_1d(menu);i++)
{
    draw_set_color(c_black);//Shadow color
    draw_set_font(fnt_zekmodos);
    draw_text(menu_x+2,menu_y+2,menu[i]);//Shadow
    draw_set_color(c_white); //Text color    
    if point_in_rectangle(mouse_x,mouse_y,menu_x-string_width(menu[i])/2,menu_y-string_height(menu[i])/2,menu_x+string_width(menu[i])/2,menu_y+string_height(menu[i])/2)
    {
        draw_set_color(c_blue); //Text color (Mouse tip)
        if mouse_check_button_pressed(mb_left)    
        {
            switch(i)
            {
                
                case 2: 
                    global.modo=2;
                    global.name = " "; 
                    global.name = get_string("Ingresa tu nombre:", "")

                 if !is_undefined((global.dsmap[? global.name])){
                    if(show_question("Ese nick ya existe, ¿Seguro que quieres iniciar sesión con este nick?")){
                        k=0
                        while(global.leader[k]!=global.name){
                            k++
                        }
                        global.leader[k] = global.name
                        room_goto(rm_select);
                        break;
                    }else{
                    room_goto(rm_modo)
                    break;
                    }
                 }else{
                 ds_map_add(global.dsmap, global.name, 0)
                 global.leader[global.contador] = global.name
                 global.contador += 1
                 }
                 room_goto(rm_select); //menu[0]
                 global.snd=audio_play_sound(music,10,true);
                 audio_sound_gain(global.snd,0.4,0);
        
                    
                break;
                
                
                // room_goto(rm_score)
                 
                //break;
                    

            }
        }
    }
    draw_text(menu_x,menu_y,menu[i]);//Text
    menu_y+=string_height(menu[i])+line_spacing;
}
</string>
